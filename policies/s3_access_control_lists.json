{
  "policy": {
    "name": "s3_bucket_access",
    "short_description": "S3 Bucket Access",
    "description": "Validate permissions on S3 buckets to ensure they are not exposing the contents to the general public",
    "settings": {
      "tests": {
        "output_format": null
      }
    },
    "operating_system_family_id": 14,
    "operating_system_id": 1416,
    "type": null
  },
  "data": [{
    "Permissions": [{
      "All": [{
        "S3": {
          "name": "*"
        },
        "id": "PermissionsAllItem-mapping-to-Permissions-All-should-contain-the-defined-values",
        "name": "Item mapping to Permissions,All should not contain the defined values",
        "checks": {
          "*": [{
            "exp": "http://acs.amazonaws.com/groups/global/AllUsers",
            "check": "excludes",
            "expected": "http://acs.amazonaws.com/groups/global/AllUsers",
            "background": "Checks the bucket is not exposed to the public internet"
          }, {
            "exp": "http://acs.amazonaws.com/groups/global/AuthenticatedUsers",
            "name": "*",
            "path": ["Permissions", "All", "Item mapping to Permissions,All should contain the defined values"],
            "check": "excludes",
            "expected": "http://acs.amazonaws.com/groups/global/AuthenticatedUsers",
            "background": "Checks the bucket is not exposed to any authenticated AWS user"
          }]
        },
        "ci_path": ["Permissions", "All"],
        "check_type": "s3",
        "selectList": ["FULL_CONTROL", "Owner", "READ", "READ_ACP", "WRITE", "WRITE_ACP", "*"],
        "nodeGroupsOpen": true
      }]
    }]
  }],
  "scan_options": {}
}
